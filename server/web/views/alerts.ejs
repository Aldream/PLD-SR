<ul class="breadcrumb">
	<li>
		<a href="index">Home</a> <span class="divider">/</span>
	</li>
	</li>
		<li class="active">
		<a href="#">Notifications</a>
	</li>
</ul>

<div class="hero-unit">
<h2>Notifications</h2>
<h3>Activite recente</h3>
<table class="table table-condensed">
    <thead>
        <tr>
        <th>Description</th>
        <th>Heure</th>
        </tr>
    </thead>
    <tbody>
		<% for(var i in notifs.alerts) {
			var a = notifs.alerts[i];
			notifTime = dateToString(new Date(a.time/1000)) + " : "; %>
			<tr><td><i class="icon-time"></i>&nbsp;<%= a.name %></td> <td><%= notifTime %></td></tr>
		<% } %>
    </tbody>
</table>
</div>
